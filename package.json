  "scripts": {
    "play": "pnpm run --filter ./play dev",
    "build:element-plus-expand": "gulp -f packages/element-plus-expand/component/script/build/index.ts",
    "publish:element-plus-expand": "./login.sh && gulp -f packages/element-plus-expand/component/script/publish/index.ts && ./end.sh && ./packages/element-plus-expand/browser.sh",
    "docs:dev": "vitepress dev site",
    "docs:build": "pnpm run --filter ./site build",
    "docs:deploy": "pnpm run --filter docs deploy",
